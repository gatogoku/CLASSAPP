<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.classapp.backend.data.model">

	<class name="Class" table="CLASS">
		<id name="id" column="ID">
			<generator class="identity" />
		</id>
		<property name="name" column="NAME"></property>
		<property name="description" column="DESCRIPTION"></property>
		<property name="since" column="SINCE"></property>
		<property name="latitude" column="LATITUDE"></property>
		<property name="longitude" column="LONGITUDE"></property>
		<property name="idUser" column="IDUSER"></property>

	</class>

</hibernate-mapping>